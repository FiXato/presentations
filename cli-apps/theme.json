{
  "document": {
    "block_prefix": "\n\n",
    "block_suffix": "",
    "color": "252",
    "margin": 6
  },
  "block_quote": {
    "color": "7",
    "indent_token": "│ ",
    "margin": 0,
    "indent": 1,
    "italic": true
  },
  "paragraph": {},
  "list": {
    "level_indent": 2
  },
  "heading": {
    "block_suffix": "\n",
    "color": "39",
    "bold": true
  },
  "h1": {
    "prefix": "~~ ",
    "suffix": " ~~",
    "color": "#9fc",
    "bold": true
  },
  "h2": {
    "prefix": "▓ ",
    "color": "#1cc"
  },
  "h3": {
    "prefix": "",
    "color": "253"
  },
  "h4": {
    "color": "7",
    "prefix": "",
    "underline": true
  },
  "h5": {},
  "h6": {},
  "text": {},
  "strikethrough": {
    "crossed_out": true
  },
  "emph": {
    "italic": true
  },
  "strong": {
    "color": "#fff",
    "bold": true
  },
  "hr": {
    "color": "240",
    "format": "\n--------\n"
  },
  "item": {
    "block_prefix": "   ",
    "margin": 1
  },
  "enumeration": {
    "block_prefix": ". "
  },
  "task": {
    "ticked": "[✓] ",
    "unticked": "[ ] "
  },
  "link": {
    "color": "30",
    "underline": true
  },
  "link_text": {
    "color": "35",
    "bold": true
  },
  "image": {
    "color": "212",
    "underline": true
  },
  "image_text": {
    "color": "243",
    "format": "Image: {{.text}} →"
  },
  "code": {
    "prefix": " ",
    "suffix": " ",
    "background_color": "#2c3147"
  },
  "code_block": {
    "block_prefix": "╭─  ────────────────────────────────────────────────────────────────────────────────╮\n\n",
    "block_suffix": "\n╰────────────────────────────────────────────────────────────────────────────────────╯\n",
    "theme": "solarized-dark"
  },
  "table": {
    "center_separator": "┼",
    "column_separator": "│",
    "row_separator": "─"
  },
  "definition_list": {},
  "definition_term": {},
  "definition_description": {
    "block_prefix": "\n🠶 "
  },
  "html_block": {},
  "html_span": {}
}
